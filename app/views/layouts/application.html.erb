<!DOCTYPE html>
<html>
<head>
  <title>TestLine</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="content">
    <% if flash[:alert] %>
      <div class="alert"><%= flash[:alert ] %></div>
    <% end %>
    <%= yield %>
  </div>
</body>
</html>
